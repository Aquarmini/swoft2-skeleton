{
    "name": "limingxinleo/swoft2-skeleton",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "swoft"
    ],
    "description": "Modern High performance AOP and Coroutine PHP Framework, base on Swoole",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.2",
        "ext-swoole": "^4.3.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "swoft/annotation": "^2.0",
        "swoft/bean": "^2.0",
        "swoft/event": "^2.0",
        "swoft/aop": "^2.0",
        "swoft/config": "^2.0",
        "swoft/stdlib": "^2.0",
        "swoft/framework": "^2.0",
        "swoft/http-message": "^2.0",
        "swoft/server": "^2.0",
        "swoft/tcp-server": "^2.0",
        "swoft/http-server": "^2.0",
        "swoft/websocket-server": "^2.0",
        "swoft/log": "^2.0",
        "swoft/db": "^2.0",
        "swoft/connection-pool": "^2.0",
        "swoft/test": "^2.0",
        "swoft/console": "^2.0",
        "swoft/rpc": "^2.0",
        "swoft/rpc-server": "^2.0",
        "swoft/rpc-client": "^2.0",
        "swoft/task": "^2.0",
        "swoft/proxy": "^2.0",
        "swoft/error-handler": "^2.0",
        "swoft/component": "2.0.x-dev as 2.0"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.10",
        "psy/psysh": "@stable"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/swoft-cloud/swoft-component.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    ]
}